{
  "name": "express-template",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "transpile": "babel src --out-dir dist -s",
    "build": "NODE_ENV=production rm -rf dist && yarn migrate && yarn transpile",
    "start": "NODE_ENV=production node dist/index.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "lint": "./node_modules/.bin/eslint src",
    "test": "NODE_ENV=test yarn migrate && jest --forceExit --coverage",
    "start:dev": "NODE_ENV=development nodemon --exec babel-node src/index.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@babel/register": "^7.4.4",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-validator": "^5.3.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.10.0",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "babel-eslint": "^10.0.2",
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  }
}
